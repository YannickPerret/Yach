datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Calendar {
  id        String   @id @default(uuid())
  name      String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  events    Event[] 
}

model Event {
  uuid          String   @id @default(uuid())
  createdAt     DateTime @default(now())
  updatedAt     DateTime @updatedAt
  summary       String
  sequence      String
  status        String
  transp        String
  rRule         String
  start         DateTime
  end           DateTime
  drStamp       DateTime
  categories    String
  location      String
  geo           String
  description   String
  url           String
  calendarId    String 
  calendar      Calendar @relation(fields: [calendarId], references: [id])
}

